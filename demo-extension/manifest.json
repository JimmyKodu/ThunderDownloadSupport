{
  "manifest_version": 3,
  "name": "Download Interception Demo",
  "version": "1.0.0",
  "description": "演示如何拦截浏览器下载 - Demo of browser download interception",
  "permissions": [
    "downloads",
    "webRequest",
    "cookies",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
